<h1>Listing relationships</h1>

<table>
  <tr>
    <th>User</th>
    <th>Followed user</th>
    <th>Auto</th>
    <th>Invisible</th>
    <th>Blocked</th>
    <th>From</th>
    <th>Found on other network</th>
    <th>Referred signup</th>
    <th>Referred signup via</th>
    <th>Referrred by budge request</th>
    <th>Referred by budge</th>
    <th>Num given budges</th>
    <th>Num accepted budges</th>
    <th>Num successful budges</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @relationships.each do |relationship| %>
  <tr>
    <td><%= relationship.user_id %></td>
    <td><%= relationship.followed_user_id %></td>
    <td><%= relationship.auto %></td>
    <td><%= relationship.invisible %></td>
    <td><%= relationship.blocked %></td>
    <td><%= relationship.from %></td>
    <td><%= relationship.found_on_other_network %></td>
    <td><%= relationship.referred_signup %></td>
    <td><%= relationship.referred_signup_via %></td>
    <td><%= relationship.referrred_by_budge_request_id %></td>
    <td><%= relationship.referred_by_budge_id %></td>
    <td><%= relationship.num_given_budges %></td>
    <td><%= relationship.num_accepted_budges %></td>
    <td><%= relationship.num_successful_budges %></td>
    <td><%= link_to 'Show', relationship %></td>
    <td><%= link_to 'Edit', edit_relationship_path(relationship) %></td>
    <td><%= link_to 'Destroy', relationship, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Relationship', new_relationship_path %>
